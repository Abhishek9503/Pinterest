<% include ./partials/header.ejs %>
<div class="w-full min-h-screen bg-zinc-800 py-8 px-10 text-white">
    <h1 class=" text-5xl mb-4 text-4xl font-extrabold leading-none tracking-tight  md:text-5xl lg:text-6xl">Feed Section</h1>
  <div class="mt-10 cards flex flex-wrap gap-10">
    <% posts.forEach(function(elem){ %>
    <div class="card w-32">
      <div class="w-full h-40 bg-zinc-700 rounded-lg overflow-hidden">
        <img
          class="w-full h-full object-cover"
          src="/images/uploads/<%=elem.image%>"
          alt=""
        />
      </div>
      <h5 class="mt-2 test-sm"><%= elem.title %></h5>
      <h6 class="font-semibold opacity-20 test-xs">
        <%= elem.user.username %>
      </h6>
    </div>
    <%})%>
  </div>
</div>
<% include ./partials/footer.ejs %>
